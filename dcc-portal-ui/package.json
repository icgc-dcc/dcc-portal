{
  "name": "dcc-portal-ui",
  "description": "ICGC DCC Data Portal UI",
  "version": "4.2.6-SNAPSHOT",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git://github.com:icgc-dcc/dcc-portal.git"
  },
  "scripts": {
    "sethost": "hostile set localhost local.dcc.icgc.org",
    "unsethost": "hostile remove local.dcc.icgc.org",
    "checkhost": "node tasks/checkDns.js",
    "predev": "npm run checkhost",
    "dev": "grunt server",
    "dev:prodapi": "API_SOURCE=production npm run dev",
    "font:open": "fontello-cli --config app/styles/fonts/config.json open",
    "test-bower": "rm -rf app/bower_components/ && rm -rf ~/.cache/bower/ && bower install -V 2>&1 | tee test-bower-$(date +%Y%m%d%H%M%S).txt && cat app/bower_components/angular/bower.json | grep version"
  },
  "devDependencies": {
    "autoprefixer": "6.3.7",
    "bower-art-resolver": "2.0.5",
    "chalk": "1.1.3",
    "connect-livereload": "0.5.0",
    "connect-modrewrite": "0.5.6",
    "fontello-cli": "0.4.0",
    "grunt": "0.4.5",
    "grunt-bower-install-simple": "1.2.3",
    "grunt-concurrent": "0.3.0",
    "grunt-connect-proxy": "git+https://github.com/drewzboto/grunt-connect-proxy.git#f798bbd31b76b039a392b8f1bca55b310a7ac5c9",
    "grunt-contrib-clean": "0.4.1",
    "grunt-contrib-coffee": "0.7.0",
    "grunt-contrib-compass": "0.3.0",
    "grunt-contrib-concat": "0.5.1",
    "grunt-contrib-connect": "0.8.0",
    "grunt-contrib-copy": "0.7.0",
    "grunt-contrib-cssmin": "0.11.0",
    "grunt-contrib-htmlmin": "0.3.0",
    "grunt-contrib-imagemin": "0.9.4",
    "grunt-contrib-jshint": "0.11.3",
    "grunt-contrib-uglify": "0.11.1",
    "grunt-contrib-watch": "0.6.1",
    "grunt-filerev": "2.3.1",
    "grunt-google-cdn": "0.2.0",
    "grunt-injector": "0.6.1",
    "grunt-jsdoc-to-markdown": "1.2.1",
    "grunt-karma": "2.0.0",
    "grunt-newer": "1.2.0",
    "grunt-ng-annotate": "2.0.2",
    "grunt-notify": "0.4.5",
    "grunt-open": "0.2.0",
    "grunt-peg": "1.5.0",
    "grunt-postcss": "0.8.0",
    "grunt-usemin": "3.0.0",
    "hostile": "1.0.2",
    "http-proxy": "0.10.0",
    "jasmine-core": "2.4.1",
